    "eslint-config-mongodb-js": "^2.0.0",
    "eslint-plugin-react": "^5.2.2",
    "mocha": "^2.5.3",
    "mongodb-js-fmt": "^0.0.3",
    "mongodb-js-precommit": "^0.2.8",
    "pre-commit": "^1.1.3"
